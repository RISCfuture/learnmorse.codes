    "lint": "run-s 'lint:*'",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:dev": "playwright test --ui",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build"
  },
  "dependencies": {
