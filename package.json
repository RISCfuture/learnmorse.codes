{
  "name": "learn-morse-code",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit -i tests/unit/setup.ts",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "docs:generate": "npx typedoc --out docs src",
    "ci:e2e:start-server": "yarn serve",
    "ci:e2e:run": "cypress run --record --key 5d510140-7407-4f81-b342-49a54e5a00b6",
    "ci:e2e": "npx start-server-and-test ci:e2e:start-server 8080 ci:e2e:run",
    "ci:unit": "yarn run test:unit"
  },
  "dependencies": {
    "@bugsnag/js": "^7.9.0",
    "@bugsnag/plugin-vue": "^7.9.0",
    "audiocontext": "^0.1.0",
    "core-js": "^3.9.1",
    "dom-confetti": "^0.2.2",
    "element-in-view": "^0.1.0",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "scroll-behavior-polyfill": "^2.0.13",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-i18n": "^8.24.2",
    "vue-property-decorator": "^9.1.2",
    "vuex": "^3.6.2",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.6.0",
    "@types/chai": "^4.2.15",
    "@types/chai-almost": "^1.0.1",
    "@types/chai-as-promised": "^7.1.3",
    "@types/lodash-es": "^4.17.4",
    "@types/mocha": "^8.2.2",
    "@types/sinon": "^9.0.11",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vue/cli-plugin-babel": "~4.5.12",
    "@vue/cli-plugin-e2e-cypress": "~4.5.12",
    "@vue/cli-plugin-eslint": "~4.5.12",
    "@vue/cli-plugin-router": "~4.5.12",
    "@vue/cli-plugin-typescript": "~4.5.12",
    "@vue/cli-plugin-unit-mocha": "~4.5.12",
    "@vue/cli-plugin-vuex": "~4.5.12",
    "@vue/cli-service": "~4.5.12",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "1.1.3",
    "chai": "^4.3.4",
    "chai-almost": "^1.0.1",
    "chai-as-promised": "^7.1.1",
    "chai-each": "^0.0.1",
    "eslint": "^7.23.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-vue": "^7.8.0",
    "lint-staged": "^10.5.4",
    "null-loader": "^4.0.1",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "start-server-and-test": "^1.12.1",
    "stylelint": "^13.12.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-processor-html": "^1.0.0",
    "typedoc": "^0.20.34",
    "typescript": "~4.2.3",
    "vue-template-compiler": "^2.6.12"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint --fix"
    ],
    "*.{css,vue}": [
      "npx stylelint --fix"
    ],
    "*.scss": [
      "npx stylelint --fix --syntax scss"
    ],
    "*.sass": [
      "npx stylelint --fix --syntax sass"
    ]
  }
}
